#include <stdio.h>

int main(){
    int n ,i,a;
    printf("Enter the number of rows and coloums in the matrix :");
    scanf("%d %d",&n,&a);
    
    int f[n][a] ;
    
    printf("Enter all the elemants in the coloum");
    for(int j=0;j<n;j++){
    for(int i=0;i<a;i++){
        scanf("%d",&f[j][i]);}
        
    }
 
      for(int j=0;j<n;j++){
    for(int i=0;i<a;i++){
      printf("%d",f[j][i]);
    }
    printf("\n");
    
}
return 0;
}
