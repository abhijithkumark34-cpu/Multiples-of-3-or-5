// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n;
    printf("Enter the number of digit in the array");
    scanf("%d",&n);
    int a[n];
    printf("Enter the all number");
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++){
        int min=i;
    
        for(int j=i+1;j<n;j++){
            if(a[min]>a[j]){
                min=j;
            
            }
        }
        if(min!=i){
        int temp=a[i];
        a[i]=a[min];
        a[min]=temp;
        }
    
    }
        for(int i=0;i<n;i++){
        printf("%d",a[i]);}
    return 0;
}
