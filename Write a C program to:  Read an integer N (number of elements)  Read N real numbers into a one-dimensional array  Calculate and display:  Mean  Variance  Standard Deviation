
#include <stdio.h>

int main() {
    int n ,sum=0;
    printf("Enter the number");
    scanf("%d",&n);
     int e[n];
        printf("Enter the elemants");
        for(int i=0;i<n;i++){
        
        scanf("%d",&e[i]);
        sum=sum+e[i];}
        float mean=(float)sum/n;
            
        
         printf("%f\n",mean);
         float sum2=0;
      for (int s=0;s<n;s++){
          
           sum2=sum2+(e[s]-mean)*(e[s]-mean);
           
          
          
      }
        float  var=sum2/n;
      printf("%f",var);
       
        
    return 0;
}
